    <div class="mdui-dialog" id="option-tool">
        <div class="mdui-dialog-title mdui-color-theme">
            <span class="mdui-typo-headline">设置</span>
            <div class="mdui-typo-caption-opacity">你可以通过这个工具修改网站设置</div>
        </div>
        <div class="mdui-divider-dark" style="height: 2px;"></div>
        <div class="mdui-dialog-content">
            <div class="mdui-row">
                <div class="mdui-col-xs-12 mdui-p-b-1">
                    <div class="mdui-typo-title">语言 | Language</div>
                </div>
                <div class="mdui-col-xs-1 mdui-p-b-1" style="width: 30px;"></div>
                <div class="mdui-col-xs-4 mdui-p-b-1 mdui-typo">设置页面语言</div>
                <div class="mdui-col-xs-4 mdui-p-b-1 mdui-text-right">
                    <select class="mdui-select" mdui-select>
                    <%_ Object.keys(AVAILABLE_LANGUAGES).forEach(lang => { -%>
                        <option value="<%= lang %>"<%= (()=>{if(LANG===lang)return" selected"})() %>><%= AVAILABLE_LANGUAGES[lang] %></option>
                    <%_ }) -%>
                    </select>
                </div>
                <div class="mdui-col-xs-12 mdui-p-b-1">
                    <div class="mdui-typo-title">主题 | Theme</div>
                </div>
                <div class="mdui-col-xs-1 mdui-p-b-1" style="width: 30px;"></div>
                <div class="mdui-col-xs-11 mdui-p-b-1">
                    
                </div>
                <div class="mdui-col-xs-12 mdui-p-b-1">
                    <div class="mdui-typo-title">加载 | Load</div>
                </div>
                <div class="mdui-col-xs-1 mdui-p-b-1" style="width: 30px;"></div>
                <div class="mdui-col-xs-11 mdui-p-b-1">
                    
                </div>
            </div>
        </div>
        <div class="mdui-dialog-actions">
            <button class="mdui-btn mdui-ripple" onclick="">保存</button>
            <button class="mdui-btn mdui-ripple" mdui-dialog-close>关闭</button>
        </div>
    </div>